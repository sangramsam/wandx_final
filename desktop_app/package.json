{
  "name": "WandX-Desktop-App(beta)",
  "version": "1.0.0",
  "license": "MIT",
  "main": "main-electron.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start-https": "ng serve --port 4200",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --prod",
    "pack": "build --dir",
    "dist": "build",
    "electron-pack": "build -w",
    "dist-win": "build --win",
    "dist-linux": "build --linux",
    "dist-mac": "build --mac",
    "generate": "ng generate component profile-container",
    "postinstall": "electron-rebuild --force; mkdir tempFolder; cd tempFolder; git init; git remote add origin https://gitlab.com/ankandasnew1/0x-relayers-dependency.git; git pull origin master; cd ..; rm -rf tempFolder/.git; rm -rf tempFolder/README.md; rsync -avu tempFolder/ node_modules; rm -rf tempFolder;"
  },
  "build": {
    "appId": "com.hypermodules.testapp",
    "productName": "WandX Dapp",
    "files": [
      "main-electron.js",
      "icons",
      "assets",
      "config",
      "splash"
    ],
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "icons/mac/wandx_logo.icns",
      "target": [
        "dmg"
      ]
    },
    "dmg": {
      "background": "assets/background.tiff",
      "icon": "icons/mac/wandx_logo.icns",
      "iconSize": 100,
      "window": {
        "width": 544,
        "height": 408
      },
      "contents": [
        {
          "x": 145,
          "y": 257
        },
        {
          "x": 400,
          "y": 255,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "icons/win/wandx_logo.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icons/mac/64x64.png",
      "publish": [
        "github"
      ]
    }
  },
  "homepage": "https://gitlab.com/pravin-d/dw#README",
